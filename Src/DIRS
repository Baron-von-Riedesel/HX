
# HXINST:  location where to install HX binaries
# LIBCOFF: location where COFF libraries will be read from

HXINST=\hx
LIBCOFF=$(HXINST)\Lib
INC32DIR=..\..\Include
INC16DIR=.

MAKE=nmake.exe /nologo
#MAKE=wmake.exe -h -ms
ASM=jwasm.exe
LINKBIN=jwlink.exe
LINK16BIN=jwlink.exe
LIBBIN=jwlib.exe -q -fa -n
LIB16BIN=jwlib.exe -q -b -n
